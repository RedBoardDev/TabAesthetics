{
  "manifest_version": 3,
  "name": "TabAesthetics",
  "version": "1.0.3",
  "description": "Sleek, modern design for the new chrome tab",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "favicon",
    "history"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "_favicon/*"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": [
        "*"
      ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}